<%= form_for @study, url: {action: "create"} do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description, size: "60x12" %>
  </div>
  <div class="field">
    <%= f.label :start %><br />
    <%= f.datetime_field :start, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :end %><br />
    <%= f.datetime_field :end, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
